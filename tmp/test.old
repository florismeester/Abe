package main

import (
    "fmt"
    "os"
    "path/filepath"
)

func main() {
    searchDir := "/usr"

    fileList := []string{}
    err := filepath.Walk(searchDir, func(path string, f os.FileInfo, err error) error{
        fileList = append(fileList, path)
        return nil
    })
	fmt.Println(fileList)

    for _, file := range fileList {
	if file == "" {
        fmt.Println(file)
	}
    }
        if err != nil {
                fmt.Println(err)
        }
}
